        "show": true,
        "position": "Center Right",
        "labelColor": {"solid": {"color": "#000000"} },
        "fontFamily": "Arial",
        "fontSize": 12
        }]
     }
   },
  "slicer": {
    "*": {
      "selection": [{
        "selectAllCheckboxEnabled": true,
        "singleSelect": true
        }],
      "header": [{
        "show": true,
        "fontColor": {"solid": {"color": "#000000"} },
        "textSize": 12,
        "fontFamily": "Arial"
        }],
      "items": [{
        "fontColor": {"solid": {"color": "#000000"} },
        "textSize": 12,
        "fontFamily": "Arial"
        }]
    }
  },
  "LineChart": {
    "*": {
      "legend": [{
        "show": true,
        "labelColor": {"solid": {"color": "#000000"} },
