      "valueAxis" : [{
        "show": true,
        "start": 0,
        "labelColor": {"solid": {"color": "#000000"} },
        "fontSize": 12,
        "fontFamily": "Arial",
        "gridlineShow": false
        }],
      "labels": [{
        "show": true,
        "color": {"solid": {"color": "#000000"} },
        "fontSize": 10,
        "fontFamily": "Arial"
        }],
      "legend": [{
        "show": true,
        "position": "Center Right",
        "labelColor": {"solid": {"color": "#000000"} },
        "fontFamily": "Arial",
        "fontSize": 12
        }]
    }
  },
  "clusteredBarChart": {
    "*": {
      "categoryAxis": [{
        "show": true,
        "labelColor": {"solid": {"color": "#000000"} },
        "fontFamily": "Arial",
        "fontSize": 14
        }],
      "valueAxis" :[{
        "show": true,
        "start": 0,
        "labelColor": {"solid": {"color": "#000000"} },
        "fontSize": 12,
        "fontFamily": "Arial",
        "gridlineShow": false
        }],
      "labels": [{
        "show": true,
        "color": {"solid": {"color": "#000000"} },
        "fontSize": 10,
        "fontFamily": "Arial"
        }],
